;
      RETURN;
     END (* WITH *) ;

  | Tree.NodeTypes:
(* line 1085 "" *)
     WITH t^.NodeTypes DO
(* line 1085 "" *)
      
        WriteS (f, "  IF "); WI (TheName); WriteS (f, " = "); WI (TreeName^.TreeName.Name); WriteS (f, " . No"); 
