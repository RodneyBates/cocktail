PROCEDURE TreeImplC1 (t: Tree)

Ag (..) :- {

      IF IsElement (ORD ('R'), Options) THEN
        WI (itTree); ! Reverse! WI (iModule); !!
        !# if defined __STDC__ | defined __cplusplus!
        ! (! WI (itTree); ! yyOld)!
