Ag (..) :- {
        @# include "@ WI (iMain); @.h"@
        !# define yyALLOC(ptr, size)    if ((ptr = (! WI (itTree); !) ! WI (iMain); !_PoolFreePtr) >= (! WI (itTree); !) ! WI (iMain); !_PoolMaxPtr) ! WriteBackslash ( f ) ; !!
        !  ptr = ! WI (iMain); !_Alloc (); ! WriteBackslash ( f ) ; !!
        !  ! WI (iMain); !_PoolFreePtr += size;!
        !# define yyFREE(ptr, size)     !
        !# include <stdio.h>!
        !# ifdef __cplusplus!
        @extern "C" {@
        @# include "System.h"@
        @# include "General.h"@
        @# include "Memory.h"@
        @# include "DynArray.h"@
        @# include "StringMem.h"@
        @# include "Idents.h"@
        @# include "Sets.h"@
